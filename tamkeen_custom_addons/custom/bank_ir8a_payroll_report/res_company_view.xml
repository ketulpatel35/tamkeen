<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- res.company form view -->
        <record model="ir.ui.view" id="view_company_inherit_mol">
            <field name="name">res.company.mol.form.inherit</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="model">res.company</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_registry']"
                       position="after">
                    <field name="mol_establishment_id"/>
                    <field name="connect_id"/>
                    <field name="payroll_partner_bank"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
