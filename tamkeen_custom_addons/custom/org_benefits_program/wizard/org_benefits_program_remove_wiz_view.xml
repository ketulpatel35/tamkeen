<odoo>

    <record id="org_benefits_program_remove_wizard_view" model="ir.ui.view">
        <field name="name">org.benefits.program.remove.wizard.view</field>
        <field name="model">org.benefits.program.remove.wiz</field>
        <field name="arch" type="xml">
            <form string="Remove Benefits Program Request">
                <p class="oe_grey">
                    Are you sure you want to delete the Benefits Program
                    request?
                </p>
                <footer>
                    <button string="Delete"
                            name="remove_service" type="object"
                            default_focus="1" class="btn-primary"/>
                    <button string="Cancel" class="btn-default"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="action_benefits_program_remove"
                multi="True"
                key2="client_action_multi" name="Delete My Own Request"
                res_model="org.benefits.program.remove.wiz"
                src_model="org.benefits.program"
                view_mode="form" target="new" view_type="form"
                groups="org_benefits_program.group_bp_normal_user"/>
</odoo>