<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Org End of Service Type Form View -->
    <record id="org_end_of_service_type_form_view" model="ir.ui.view">
        <field name="name">org.end.of.service.type.form.view</field>
        <field name="model">org.end.of.service.type</field>
        <field name="arch" type="xml">
            <form string="Org End of Service">
                <sheet>
                    <group colspan="4" col="4">
                        <field name="name" required="1"/>
                        <field name="code" required="1"/>
                        <field name="active"/>
                    </group>
                    <notebook>
                        <page string="Description" name="description">
                            <field name="description"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!--Org End of Service Type Tree View-->
    <record id="org_end_of_service_type_tree_view" model="ir.ui.view">
        <field name="name">org.end.of.service.type.tree.view</field>
        <field name="model">org.end.of.service.type</field>
        <field name="arch" type="xml">
            <tree string="End of Service Type">
                <field name="name"/>
                <field name="code"/>
                <field name="active"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <!-- Org End of Service Type Search View -->
    <record id="org_end_of_service_type_search_view" model="ir.ui.view">
        <field name="name">org.end.of.service.type.search.view</field>
        <field name="model">org.end.of.service.type</field>
        <field name="arch" type="xml">
            <search string="End of Service Type">
                <field name="name"/>
                <field name="code"/>
            </search>
        </field>
    </record>

    <!-- Org End of Service Type Action -->
        <!-- Org End of Service Action view -->
    <record id="org_end_of_service_type_action_view"
            model="ir.actions.act_window">
        <field name="name">End of Service Type</field>
        <field name="res_model">org.end.of.service.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <!--<field name="context"></field>-->
        <!--<field name="domain">[]</field>-->
    </record>

</odoo>