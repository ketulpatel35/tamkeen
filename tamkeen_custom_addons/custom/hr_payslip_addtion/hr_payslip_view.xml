<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="hr_payslip_addtion_inherit">
             <field name="name">hr.payslip.additon</field>
             <field name="model">hr.payslip</field>
             <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form" />
             <field name="arch" type="xml">
                <xpath expr="//field[@name='credit_note']" position="before">
                    <field name="check_employee_start_date"/>
                    <field name="first_contract_date"/>
                    <field name="number_of_days_new_employee"/>
                </xpath>
             </field>
        </record>
    </data>
</odoo>