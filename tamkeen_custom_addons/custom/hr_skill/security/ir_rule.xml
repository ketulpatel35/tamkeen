<?xml version="1.0"?>
<odoo>

     <record id="rule_hr_skill" model="ir.rule">
        <field name="name">Normal Employee</field>
        <field name="model_id" ref="hr_skill.model_hr_skill"/>
        <field name="domain_force">[('emp_line.user_id','=',user.id)
            ]</field>
        <field name="groups" eval="[(4,ref('base.group_user'))]"/>
    </record>

    <record id="rule_hr_skill_all_info" model="ir.rule">
        <field name="name">Employee all info</field>
        <field name="model_id" ref="hr_skill.model_hr_skill" />
        <field name="domain_force"> [(1, '=', 1)]</field>
        <field name="groups" eval="[(4,ref('hr_employee_customization.group_view_emp_info'))]"/>
    </record>

</odoo>