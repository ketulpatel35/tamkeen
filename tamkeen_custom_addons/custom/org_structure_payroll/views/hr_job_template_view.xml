<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<!-- HR Job Template Form View-->
    <record id="hr_job_template_form_view_inherit" model="ir.ui.view">
        <field name="name">hr.job.template.form.view.inherit</field>
        <field name="model">hr.job.template</field>
        <field name="inherit_id"
               ref="organization_structure.hr_job_template_form_view"/>
        <field name="arch" type="xml">
            <field name="employee_sub_group_id" position="after">
                <field name="struct_id" required="1" options="{'no_open': True,'no_create': True,'no_quick_create': True,'no_create_edit': True, 'limit':1000}"/>
                <field name="grade_level_id" required="1" options="{'no_open': True,'no_create': True,'no_quick_create': True,'no_create_edit': True, 'limit':1000}"/>
            </field>
        </field>
    </record>
</odoo>