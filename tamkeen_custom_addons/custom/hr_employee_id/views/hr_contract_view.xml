<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="hr_contract_view_inherit77">
            <field name="name">Contract Data</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
                <field name="employee_id" position="after">
                    <field name="f_employee_no" readonly="1"/>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="hr_contract_search_view_inherit77">
            <field name="name">Contract Filter</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_search"/>
            <field name="arch" type="xml">
                <field name="name" position="before">
                    <field name="f_employee_no"/>
                </field>
            </field>
        </record>
    </data>
</odoo>