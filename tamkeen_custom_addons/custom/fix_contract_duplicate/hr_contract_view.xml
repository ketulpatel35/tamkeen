<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_contract_form" model="ir.ui.view">
            <field name="name">hr.contract.form.inherit.2</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_employee_state.view_contract_form" />
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//button[@name='end_contract']" position="after">
                        <button name="to_complate" states="contract_ending" string="Complate contract"
                                type="object" groups="fix_contract_duplicate.group_contract_duplicate_manager" />
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>
