        <!-- Report main menu -->
        <menuitem name="Reports" id="overtime_reports_menu"
                  parent="overtime_menu_root" sequence="3"
                  groups="hr_overtime.group_overtime_xls_report"/>

        <!-- overtime report sub menu -->
        <menuitem name="Overtime Report Generation" id="overtime_excel_report"
                  parent="overtime_reports_menu" sequence="1"
                  action="wizard_print_overtime_xls_report"
                  groups="hr_overtime.group_overtime_xls_report"/>
