<odoo>
    <data>

        <!-- overtime main menu -->
        <menuitem name="Overtime Management" id="overtime_menu_root"
                  groups="hr_overtime.group_overtime_view_all_overtime,hr_overtime.group_overtime_hr_approval,hr_overtime.group_overtime_budget_approval,hr_overtime.group_overtime_config,hr_overtime.group_overtime_xls_report"
