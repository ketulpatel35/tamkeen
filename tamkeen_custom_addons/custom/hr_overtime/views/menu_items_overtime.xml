                  action="open_overtime_pre_request_for_budget"
                  groups="hr_overtime.group_overtime_budget_approval"/>
        <menuitem name="View All Requests" id="overtime_pre_menu_view_all"
                  parent="overtime_pre_request_menu_child" sequence="4"
                  action="open_overtime_pre_request_for_view_all"
                  groups="hr_overtime.group_overtime_view_all_overtime"/>

        <!-- overtime statement request -->
        <menuitem name="Overtime Claims"
                  id="overtime_request_menu"
                  parent="overtime_management_menu" sequence="5"
                  groups="hr_overtime.group_overtime_view_all_overtime,hr_overtime.group_overtime_hr_approval,hr_overtime.group_overtime_budget_approval"/>

        <menuitem name="HR Approval" id="overtime_request_for_hr"
