        <menuitem name="Configuration" id="overtime_configration"
                  groups="hr_overtime.group_overtime_config"
                  sequence="2" parent="overtime_menu_root"/>

        <menuitem name="Overtime Configuration Panel"
                  id="overtime_configration_panel"
                  sequence="1"
                  action="act_open_overtime_configuration_panels_view"
                  groups="hr_overtime.group_overtime_config"
                  parent="overtime_configration"/>

        <menuitem name="Stages" id="overtime_configuration_stages"
                  sequence="2"
                  action="act_open_overtime_panel_displayed_stages_view"
                  groups="hr_overtime.group_overtime_config"
                  parent="overtime_configration"/>

